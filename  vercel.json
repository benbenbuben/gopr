{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    } 
  ],
  "routes": [
    {
      "src": "/write",
      "dest": "main.go",
      "methods": ["GET"]
    },
    {
      "src": "/read",
      "dest": "main.go",
      "methods": ["GET"]
    }
  ],
  "build": {
    "env": {
      "GO_VERSION": "1.16"
    }
  }
}